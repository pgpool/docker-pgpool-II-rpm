[pgpool44]
name=pgpool-II 4.4 $releasever - $basearch
baseurl=http://www.pgpool.net/yum/rpms/4.4/redhat/rhel-$releasever-$basearch
enabled=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-PGPOOL2

[pgpool44-source]
name=pgpool-II 4.4 $releasever - $basearch - Source
failovermethod=priority
baseurl=http://www.pgpool.net/yum/srpms/4.4/redhat/rhel-$releasever-$basearch
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-PGPOOL2
