enabled=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-PGPOOL2

[pgpool43-source]
name=pgpool-II 4.3 $releasever - $basearch - Source
baseurl=https://www.pgpool.net/yum/srpms/4.3/redhat/rhel-$releasever-$basearch
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-PGPOOL2
